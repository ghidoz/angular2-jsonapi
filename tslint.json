{
  "extends": "tslint-config-airbnb",
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "rules": {
    "align": [true, "statements"],
    "function-name": false,
    "max-line-length": [true, 120],
    "no-increment-decrement": false,
    "no-var-self": false,
    "prefer-array-literal": false,
    "ter-arrow-parens": [true, "always"],
    "ter-indent": [true, 2, {
      "SwitchCase": 1
    }],
    "trailing-comma": false
  }
}
